{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": ["/favicon.ico", "/index.html", "/*.css", "/*.js"]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": ["/assets/**"]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "api-freshness",
      "urls": [
        "https://fcuwebapi.azurewebsites.net/api/nextgame/",
        "https://fcuwebapi.azurewebsites.net/api/previousgame/",
        "https://fcuwebapi.azurewebsites.net/api/ns/stats//getShape",
        "https://fcuwebapi.azurewebsites.net/api/ranking",
        "https://fcuwebapi.azurewebsites.net/api/games/"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "3d",
        "timeout": "10s"
      }
    },
    {
      "name": "api-performance",
      "urls": [
        "https://fcuwebapi.azurewebsites.net/api/articles",
        "https://fcuwebapi.azurewebsites.net/api/ns/teamplayer/getplayers/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "3d"
      }
    }
  ]
}
